<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER">
         <children>
            <Label text="Statystyki opisowe">
               <font>
                  <Font size="35.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets top="5.0" />
         </padding>
      </HBox>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="statisticTable" prefHeight="609.0" prefWidth="1284.0" HBox.hgrow="ALWAYS">
              <columns>
                  <TableColumn fx:id="titleCol" editable="false" prefWidth="75.0" text="Miara" />
                <TableColumn fx:id="wheatCol" editable="false" text="Pszenica" />
                <TableColumn fx:id="ryeCol" editable="false" prefWidth="75.0" text="Żyto" />
                  <TableColumn fx:id="barleyCol" editable="false" prefWidth="75.0" text="Jęczmień" />
                  <TableColumn fx:id="oatCol" editable="false" prefWidth="75.0" text="Owies" />
                  <TableColumn fx:id="potatoesCol" editable="false" prefWidth="75.0" text="Ziemniaki" />
                  <TableColumn fx:id="sugarBeetsCol" editable="false" prefWidth="75.0" text="Buraki Cukrowe" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
