                  <TableColumn fx:id="ryeCol" minWidth="-1.0" onEditCommit="#cellEditCommit" prefWidth="-1.0" text="Żyto" />
                  <TableColumn fx:id="barleyCol" minWidth="-1.0" onEditCommit="#cellEditCommit" prefWidth="-1.0" text="Jęczmień" />
                  <TableColumn fx:id="oatsCol" minWidth="-1.0" onEditCommit="#cellEditCommit" prefWidth="-1.0" text="Owies" />
