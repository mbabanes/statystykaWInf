<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.WykresController">
   <children>
      <TabPane tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Grafika">
               <content>
                  <VBox prefHeight="400.0" prefWidth="600.0" spacing="10.0">
                     <children>
                        <HBox fx:id="hbox" spacing="5.0">
                           <children>
                              <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#mouseEventMapa" pickOnBounds="true" preserveRatio="true" />
                           </children>
                        </HBox>
                        <VBox prefHeight="200.0" prefWidth="100.0" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </content>
          </Tab>
          <Tab closable="false" text="Statystyka" />
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
